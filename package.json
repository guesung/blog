{
  "name": "guesung-library",
  "private": true,
  "packageManager": "pnpm@8.9.0",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev --concurrency 15",
    "dev:blog": "turbo dev --filter blog",
    "dev:docs": "turbo dev --filter docs",
    "dev:playground": "turbo dev --filter playground",
    "lint": "turbo lint",
    "format": "prettier --write **",
    "make-graph": "turbo run build lint --graph",
    "script": "ts-node scripts/example.ts"
  },
  "dependencies": {
    "clsx": "^2.1.0",
    "next": "^14.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.2"
  },
  "devDependencies": {
    "@guesung/eslint-config": "workspace:*",
    "@guesung/prettier-config": "workspace:*",
    "@guesung/typescript-config": "workspace:*",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "turbo": "latest",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "typescript": "^5.2.2"
  }
}